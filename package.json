{
  "name": "learning-object-service",
  "version": "1.2.1",
  "description": "Learning Object Microservice.",
  "main": "app.ts",
  "scripts": {
    "start": "npm run gulp-tsc",
    "build": "node_modules/.bin/gulp tsc",
    "test": "node_modules/.bin/jest --verbose  --coverage",
    "gulp-tsc": "node_modules/.bin/gulp start",
    "pack": "npm run build && cp ./package.json ./dist && cp -r ./.ebextensions ./dist && cd ./dist && zip -r ../dist.zip  ./* & cd ..",
    "zip": "cp ./package.json ./dist && cp -r ./.ebextensions ./dist && cd ./dist && zip -r ../dist.zip  ./* & cd ..",
    "deploy": "eb deploy LearningObject-Service-Production --profile clark --label Learning-Object-Service-v$(node -p \"require('./package.json').version\")"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cyber4All/learning-object-service.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Cyber4All/learning-object-service/issues"
  },
  "homepage": "https://github.com/Cyber4All/learning-object-service#readme",
  "dependencies": {
    "@cyber4all/clark-entity": "^2.0.5",
    "@cyber4all/clark-schema": "^2.0.0-beta.1.0",
    "@cyber4all/clark-taxonomy": "2.0.2",
    "@cyber4all/clark-tooltip": "^0.6.1",
    "@types/pdfkit": "^0.7.36",
    "@types/striptags": "^3.1.1",
    "aws-sdk": "^2.211.0",
    "chai": "^4.1.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "mongodb": "2.2.31",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "pdfkit": "^0.8.3",
    "raven": "^2.6.4",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "rxjs": "^6.2.1",
    "source-map-support": "^0.5.0",
    "stemmer": "^1.0.2",
    "stopword": "^0.1.10",
    "striptags": "^3.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^2.0.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.3",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.0.39",
    "@types/express-jwt": "0.0.40",
    "@types/jest": "^23.1.5",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/mongodb": "^3.1.1",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.6",
    "@types/oboe": "^2.0.28",
    "@types/raven": "^2.5.1",
    "@types/request-promise": "^4.1.39",
    "@types/stemmer": "^1.0.1",
    "@types/tape": "^4.2.31",
    "cors": "^2.8.4",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-typescript": "^4.0.2",
    "jest": "^23.4.0",
    "tape": "^4.8.0",
    "ts-jest": "^23.0.0",
    "tslint": "^5.8.0",
    "typescript": "^2.5.3"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(dist)/.*/.*(spec).(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  }
}
