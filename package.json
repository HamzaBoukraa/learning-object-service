{
  "name": "learning-object-service",
  "version": "2.22.0",
  "description": "Learning Object Microservice.",
  "main": "app.ts",
  "scripts": {
    "start": "nodemon",
    "serve": "node -r ts-node/register ./src/app.ts",
    "serve:debug": "node --inspect=5858 -r ts-node/register ./src/app.ts",
    "build": "node_modules/.bin/gulp tsc",
    "test": "node_modules/.bin/jest --verbose --coverage --detectOpenHandles",
    "gulp-tsc": "node_modules/.bin/gulp start",
    "pack": "npm run build && cp ./package.json ./dist && cp -r ./.ebextensions ./dist && cd ./dist && zip -r ../dist.zip  ./* & cd ..",
    "zip": "cp ./package.json ./dist && cp -r ./.ebextensions ./dist && cd ./dist && zip -r ../dist.zip  ./* & cd ..",
    "deploy": "eb deploy LearningObject-Service-Production --profile clark --label Learning-Object-Service-v$(node -p \"require('./package.json').version\")"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cyber4All/learning-object-service.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Cyber4All/learning-object-service/issues"
  },
  "homepage": "https://github.com/Cyber4All/learning-object-service#readme",
  "dependencies": {
    "@cyber4all/clark-taxonomy": "^4.0.2",
    "@elastic/elasticsearch": "^6.8.0",
    "@sentry/node": "^4.6.6",
    "aws-sdk": "^2.211.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "md5": "^2.2.1",
    "mongodb": "^3.1.6",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "natives": "^1.1.3",
    "node-service-module": "^1.0.2",
    "pdfkit": "^0.9.0",
    "request-promise": "^4.2.2",
    "stopword": "^0.1.10",
    "striptags": "^3.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^2.0.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.3",
    "@types/dotenv": "^4.0.2",
    "@types/elasticsearch": "^5.0.34",
    "@types/express": "^4.0.39",
    "@types/express-jwt": "0.0.40",
    "@types/jest": "^23.1.5",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/md5": "^2.1.33",
    "@types/mongodb": "^3.1.7",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.6",
    "@types/pdfkit": "^0.7.36",
    "@types/request-promise": "^4.1.39",
    "@types/striptags": "^3.1.1",
    "@types/supertest": "^2.0.7",
    "gulp": "^4.0.0",
    "gulp-nodemon": "^2.4.2",
    "gulp-typescript": "^5.0.1",
    "jest": "^24.5.0",
    "mongodb-memory-server": "^2.7.4",
    "supertest": "^3.3.0",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.8.0",
    "typescript": "^2.5.3"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
